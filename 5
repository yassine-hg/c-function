#include <stdio.h>
#include<math.h>

void function(int *x , int *y){
    int temp = *x;
    *x = *y;
    *y = temp;

}

int main(){
    int xa, ya;
    printf("Enter x : ");
    scanf("%d", &xa);

    printf("Enter y : ");
    scanf("%d" , &ya);

    function(&xa , &ya);

    printf("the swap : x = %d , y = %d\n", xa, ya);

    return 0;
}
